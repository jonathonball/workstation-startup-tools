#!/bin/bash
# Written for Guake Terminal 3.0.5
BASE_DIR=/home/jonathanb
guake --show;

guake -e "redshift -v" -r redshift;

guake --new-tab="${BASE_DIR}/project-environments/fabufor" -e "fabufor --color red --led ALL" -r lux-light

guake --new-tab="${BASE_DIR}" -e "canticle" -r canticle

guake --new-tab="${BASE_DIR}" -e "ssh dt1" -r dt1-src
guake -e "cdsrc";
guake --new-tab="${BASE_DIR}" -e "ssh dt1" -r dt1-op
guake -e "cdop";
guake --new-tab="${BASE_DIR}" -e "ssh dt1" -r dt1-mysql
guake -e "clear; MYsql";

guake --new-tab="${BASE_DIR}" -e "jclear" -r localhost

guake --hide
notify-send -i ~/.face Guake "Setup completed"
