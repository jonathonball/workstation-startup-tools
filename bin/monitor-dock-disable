#!/bin/bash

xrandr --output eDP-1  --mode 1920x1080 --pos 0x0 --rate 120 --rotate normal \
       --output DP-1   --off \
       --output HDMI-1 --off \
       --output DP-2   --off \
       --output HDMI-2 --off \
       --output DP-2-1 --off \
       --output DP-2-2 --off \
       --output DP-2-3 --off;

